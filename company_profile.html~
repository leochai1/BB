<script>
    $(document).ready(function() {
        $('.panel-heading a').click(function() {
            // Remove other "+" symbols and add "-" to the one being clicked
            $('.panel-heading a').not(this).find('span').text('+');
            var icon = $(this).find('span').text();
            if (icon === '+') {
                $(this).find('span').text('-');
            } else {
                $(this).find('span').text('+');
            }
        });
    });
</script>


